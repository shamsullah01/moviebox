{
  "name": "moviebox-clone",
  "version": "1.0.0",
  "description": "A modern, responsive movie streaming platform clone built with React and Ant Design, featuring integration with MovieBox.ng",
  "private": false,
  "keywords": [
    "react",
    "moviebox",
    "streaming",
    "movies",
    "tv-series",
    "entertainment",
    "ant-design",
    "responsive",
    "web-app"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com",
    "url": "https://github.com/yourusername"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/moviebox-clone.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/moviebox-clone/issues"
  },
  "homepage": "https://yourusername.github.io/moviebox-clone",
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.26.5",
    "axios": "^1.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "serve": "serve -s build -p 3000",
    "clean": "rm -rf build node_modules package-lock.json",
    "reinstall": "npm run clean && npm install",
    "check-updates": "npx npm-check-updates",
    "security-audit": "npm audit",
    "performance": "npm run build && npx lighthouse http://localhost:3000 --view"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
